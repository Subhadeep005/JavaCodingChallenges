package my.practice.java;

public class FindInArray {

	public static void main(String[] args) {
		int arr[] = {4,5,2,7,6,1};

		int temp=0;
		for(int i=0; i<arr.length ; i++){

		for(int j=i; j<arr.length; j++){

		if(arr[i] > arr[j]){

		temp = arr[i];
		arr[i] = arr[j];
		arr[j]=temp;
		}
		}
		System.out.println(arr[arr.length-2] +"And" + arr[arr.length-1]);
		}
	}

}
